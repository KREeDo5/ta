digraph {
	S0 [shape=circle]
	S1 [shape=circle]
	S2 [shape=circle]
	S3 [color=red shape=doublecircle]
	S4 [shape=circle]
	S5 [color=red shape=doublecircle]
	S6 [color=red shape=doublecircle]
	start [shape=point style=invis]
	start -> S0 [label="" style=solid]
	S0 -> S1 [label=b]
	S0 -> S2 [label=a]
	S1 -> S3 [label=b]
	S1 -> S4 [label=a]
	S2 -> S4 [label=b]
	S2 -> S5 [label=a]
	S3 -> S3 [label=b]
	S3 -> S4 [label=a]
	S4 -> S6 [label=b]
	S4 -> S6 [label=a]
	S5 -> S4 [label=b]
	S5 -> S5 [label=a]
	S6 -> S6 [label=b]
	S6 -> S6 [label=a]
}
