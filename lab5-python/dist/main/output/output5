digraph {
	S0 [color=red shape=doublecircle]
	S1 [color=red shape=doublecircle]
	S2 [color=red shape=doublecircle]
	S3 [color=red shape=doublecircle]
	S4 [color=red shape=doublecircle]
	S5 [color=red shape=doublecircle]
	S6 [shape=circle]
	S7 [color=red shape=doublecircle]
	S8 [shape=circle]
	start [shape=point style=invis]
	start -> S0 [label="" style=solid]
	S0 -> S1 [label=y]
	S0 -> S2 [label=x]
	S0 -> S3 [label=a]
	S1 -> S1 [label=y]
	S2 -> S4 [label=y]
	S2 -> S5 [label=x]
	S3 -> S6 [label=b]
	S3 -> S1 [label=y]
	S3 -> S5 [label=x]
	S3 -> S7 [label=a]
	S4 -> S4 [label=y]
	S6 -> S8 [label=y]
	S7 -> S1 [label=y]
	S7 -> S5 [label=x]
	S7 -> S7 [label=a]
	S8 -> S8 [label=y]
}
