digraph {
	S0 [shape=circle]
	S1 [shape=circle]
	S2 [shape=circle]
	S3 [shape=circle]
	S4 [shape=circle]
	S5 [shape=circle]
	S6 [color=red shape=doublecircle]
	S7 [color=red shape=doublecircle]
	S8 [shape=circle]
	S9 [shape=circle]
	S10 [color=red shape=doublecircle]
	S11 [shape=circle]
	start [shape=point style=invis]
	start -> S0 [label="" style=solid]
	S0 -> S1 [label="-"]
	S0 -> S1 [label=a]
	S0 -> S2 [label="."]
	S0 -> S3 [label=d]
	S1 -> S4 [label="."]
	S1 -> S5 [label=d]
	S2 -> S6 [label=d]
	S3 -> S7 [label="."]
	S3 -> S3 [label=d]
	S4 -> S8 [label=d]
	S5 -> S9 [label="."]
	S5 -> S5 [label=d]
	S6 -> S6 [label=d]
	S7 -> S10 [label=d]
	S8 -> S8 [label=d]
	S9 -> S11 [label=d]
	S10 -> S10 [label=d]
	S11 -> S11 [label=d]
}
