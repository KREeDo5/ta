digraph {
	S0 [shape=circle]
	S1 [shape=circle]
	S2 [color=red shape=doublecircle]
	S3 [color=red shape=doublecircle]
	start [shape=point style=invis]
	start -> S0 [label="" style=solid]
	S0 -> S1 [label=b]
	S0 -> S2 [label=a]
	S1 -> S1 [label=b]
	S1 -> S3 [label=a]
	S2 -> S3 [label=b]
	S2 -> S3 [label=a]
	S3 -> S3 [label=b]
}
